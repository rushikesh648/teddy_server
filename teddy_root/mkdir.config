section .data
    ; The path of the directory to create (must be null-terminated)
    dir_path db "/teddy_root", 0
    dir_path_len equ $ - dir_path

    success_msg db "Teddy root directory created successfully!", 0xa
    success_len equ $ - success_msg

    error_msg db "Error creating directory.", 0xa
    error_len equ $ - error_msg
